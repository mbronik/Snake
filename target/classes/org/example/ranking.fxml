<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #22222F;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.MainMenuController">
    <children>
        <Label text="RANKING" textFill="#00ff05">
         <font>
            <Font name="System Bold" size="54.0" />
         </font></Label>
      <Separator prefWidth="200.0" />
      <ScrollPane hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS">
         <content>
            <Label fx:id="scoreBoard" prefHeight="400.0" prefWidth="300.0" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </content>
      </ScrollPane>
        <Button fx:id="secondaryButton" onAction="#switchToPrimary" text="Go back">
         <font>
            <Font size="15.0" />
         </font>
         <VBox.margin>
            <Insets top="30.0" />
         </VBox.margin></Button>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
