<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #22222F;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.MainMenuController">
   <children>
      <Label contentDisplay="CENTER" text="SNAKE" textAlignment="CENTER" textFill="#00ff09">
         <font>
            <Font name="System Bold" size="96.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin></Label>
      <Separator prefHeight="5.0" prefWidth="208.0" />
      <Button fx:id="primaryButton" maxWidth="1.7976931348623157E308" onAction="#switchToPlay" text="Play!">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin></Button>
      <Button fx:id="primaryButton1" layoutX="40.0" layoutY="143.0" maxWidth="1.7976931348623157E308" onAction="#switchToRanking" text="Ranking">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="primaryButton11" layoutX="40.0" layoutY="200.0" maxWidth="1.7976931348623157E308" onAction="#switchToSetting" text="Settings">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="primaryButton111" layoutX="40.0" layoutY="257.0" maxWidth="1.7976931348623157E308" onAction="#exitApp" text="Exit">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
